<!-- Modal -->
<div class="modal fade" id="reference" role="dialog">
    <div class="modal-dialog modal-md">       
      <!-- Modal content-->
      <div class="modal-content">
        <div class="modal-header">
          <button type="button" class="close" data-dismiss="modal">&times;</button>
          <h4 class="modal-title">Configuracion Inicial</h4>
        </div>
        <div class="modal-body" >
          <div class="row">
            <div class="col-sm-4">
              <div class="form-group">
                <label for="reportId">Temp. esterilizado:</label>
                <input type="number" class="form-control" name="reference" id="inputRef" tabindex="1" value="<%=dataDefault[0].reference%>">
              </div>              
            </div>
            <div class="col-sm-4">
              <div class="form-group">
                <label for="reportId">Humedad inicial:</label>
                <input type="number" class="form-control" name="wet" id="inputWet"  tabindex="2" value="<%=dataDefault[0].wet%>">
              </div>              
            </div>
            <div class="col-sm-4">
              <div class="form-group">
                <label for="reportId">Tiempo de horneado:</label>
                <input type="number" class="form-control" name="timeOut" id="inputTimeOut" tabindex="3" value="<%=dataDefault[0].timeOut%>">
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 0:</label>
                <input type="number" class="form-control"  name="sensor0" step="0.01"  tabindex="4" value="<%=dataDefault[0].sensor0%>">
              </div>
              
              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 2:</label>
                <input type="number" class="form-control"  name="sensor2" step="0.01" tabindex="6" value="<%=dataDefault[0].sensor2%>">
              </div>

              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 4:</label>
                <input type="number" class="form-control"  name="sensor4" step="0.01" tabindex="8" value="<%=dataDefault[0].sensor4%>">
              </div>
            </div>
            <div class="col-md-6">
              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 1:</label>
                <input type="number" class="form-control"  name="sensor1" step="0.01" tabindex="5" value="<%=dataDefault[0].sensor1%>">
              </div>
              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 3:</label>
                <input type="number" class="form-control"  name="sensor3" step="0.01" tabindex="7" value="<%=dataDefault[0].sensor3%>">
              </div>
              
              <div class="form-group">
                <label for="timeOut" class="text-right">Variación de error sensor 5:</label>
                <input type="number" class="form-control"  name="sensor5" step="0.01" tabindex="9" value="<%=dataDefault[0].sensor5%>">
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-danger pull-left" tabindex="9" data-dismiss="modal">Cancelar</button>
          <button id="btnRef" class="btn btn-success pull-right" tabindex="10" >Actualizar</button>
        </div>
      </div>          
    </div>
  </div>
<!--End Modal-->


<!-- Modal -->
<div class="modal fade" id="copyReport" role="dialog">
  <div class="modal-dialog modal-md">       
    <!-- Modal content-->
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal">&times;</button>
        <h4 class="modal-title">Mantenimiento</h4>
      </div>
      <div class="modal-body" >
        <form action="">
          <div class="row">
            <div class="col-xs-12">
              <div class="form-group">
                <label for="reportId">Número de reporte</label>
                <input type="number" class="form-control" id="idReport" placeholder="#ID">
              </div>
              <div class="form-group">
                <label for="reportId">Fecha</label>
                <input type="text" class="form-control" id="dateReport" placeholder="1999-12-31" 
                data-inputmask="'alias': 'date'">
              </div>
              <div class="form-group">
                <label for="reportId">Hora</label>
                <input type="number" class="form-control" id="hourReport" placeholder="23">
              </div>    
            </div>
          </div>
        </form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-danger pull-left" data-dismiss="modal">Cancelar</button>
        <button type="button" class="btn btn-primary pull-right" onclick="submitReport()">Crear</button>
      </div>
    </div>          
  </div>
</div>
<!--End Modal-->